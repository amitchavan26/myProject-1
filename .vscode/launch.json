    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "app",
            "request": "launch",
            "mainClass": "com.example.Home.app",
            "projectName": "myproject"
        },
        {
            "type": "java",
            "name": "extra",
            "request": "launch",
            "mainClass": "com.example.Home.extra",
            "projectName": "myproject"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Main",
            "request": "launch",
            "mainClass": "com.example.Main",
            "projectName": "myproject",
            "vmArgs": "--module-path \"C:/javafx-sdk-21.0.7/lib\" --add-modules javafx.controls,javafx.fxml"
        }
    ]
}